<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>Cont</servlet-name>
    <servlet-class>test.Cont</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TestServlet</servlet-name>
    <servlet-class>test.TestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>URLTest</servlet-name>
    <servlet-class>controller.URLTest</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Cont</servlet-name>
    <url-pattern>/servlet/Cont</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TestServlet</servlet-name>
    <url-pattern>/servlet/TestServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>URLTest</servlet-name>
    <url-pattern>/servlet/URLTest</url-pattern>
  </servlet-mapping>
</web-app>